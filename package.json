{
  "name": "tyba-backend",
  "version": "1.0.0",
  "description": "Tyba backend",
  "scripts": {
    "dev": "cross-env debug=* NODE_PATH=./src ts-node-dev --respawn ./src/index.ts",
    "build": "rimraf dist && tsc",
    "start": "cross-env NODE_PATH=./dist node ./dist/index.js",
    "lint": "eslint --max-warnings 0 . && npm run tsc",
    "lint-fix": "eslint --fix .",
    "test": "cross-env NODE_PATH=./src mocha --require ts-node/register 'src/**/*.test.ts' --timeout 20000 --exit -r dotenv/config",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up"
  },
  "keywords": [],
  "author": "Andres Sabogal",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "helmet": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.7",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.6.2",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-array-reduce": "^1.0.62",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.2.4",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-files": "^1.1.3",
    "typescript": "4.7.4"
  }
}
